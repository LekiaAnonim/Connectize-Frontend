{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-width: none"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "manifest-file-extension": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 11.2"
  ]
}